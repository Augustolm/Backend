<h1 class="titulo">Lista de productos</h1>

<div id="product" class="listaProductos">
  {{#if existe}}
  <table>
    <thead>
      <th>Id</th>
      <th>Nombre del producto</th>
      <th>Descripcion</th>
      <th>Precio</th>
      <th>Estado</th>
      <th>Categoria</th>
      <th>Codigo</th>
      <th>Stock</th>
      <th>Url</th>
      
    </thead>
    <tbody>
      {{#each informacionProducto}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.price}}</td>
        <td>{{this.status}}</td>
        <td>{{this.category}}</td>
        <td>{{this.code}}</td>
        <td>{{this.stock}}</td>
        <td><img class="img-small" src={{this.thumbnail}} alt="img"></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{else}}
  <h1>No existen datos</h1>
  {{/if}}
</div>

<div class="boton-footer">
<a href="/realtimeproducts" class="btn btn-primary">Ir a tiempo real</a>

</div>
