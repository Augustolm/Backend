<div class="tiempo-real">

<h1>Agregar Productos</h1>


        <div class="formularioProducto">
              <form onsubmit="" >
                <input type="text"class="form-control" id="title" placeholder="Nombre del Producto" required  />
                <input type="text"class="form-control" id="description" placeholder="Descripcion" required  />
                <input type="text"class="form-control" id="status" placeholder="Estado" required  />
                <input type="text"class="form-control" id="category" placeholder="Categoria" required  />
                <input type="text"class="form-control" id="code" placeholder="Codigo" required  />
                <input type="number"class="form-control" id="stock" placeholder="Stock" required  />
                <input type="text"class="form-control" id="thumbnail" placeholder="url" required  />
                <input type="number" class="form-control" aria-label="Amount (to the nearest dollar)" id="price" placeholder="precio" required  />
                <button type="submit" class="btn btn-primary" id="boton1">Cargar Producto</button>
                </form>
        </div>     
<div class="productosList"></div>

</div>
